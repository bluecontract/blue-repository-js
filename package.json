    "@nx/vite": "20.4.0",
    "@nx/web": "20.4.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "18.16.9",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "axios": "^1.6.7",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "js-yaml": "^4.1.0",
    "jsdom": "~22.1.0",
    "nx": "20.4.0",
