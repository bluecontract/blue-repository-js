<% /* repository.ts.ejs */ %>
<% packages.forEach(({ slug, identifier }) => { -%>
import <%= identifier %> from './packages/<%= slug %>/index';
<% }); -%>
import { name, repositoryVersions } from './meta';
import type { BlueRepository } from '@blue-labs/repository-contract';

export const repository = {
  name,
  repositoryVersions,
  packages: {
<% packages.forEach(({ slug, identifier }) => { -%>
    '<%= slug %>': <%= identifier %>,
<% }); -%>
  },
} as const satisfies BlueRepository;

export default repository;
