{
  "$schema": "https://json-schema.org/schema",
  "$id": "SyncDeps",
  "title": "",
  "type": "object",
  "properties": {
    "libraryName": {
      "type": "string",
      "description": "Name of the library to sync the dependencies for",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "skipFormat": {
      "type": "boolean",
      "description": "Skip formatting files.",
      "default": false
    }
  },
  "required": ["libraryName"]
}
